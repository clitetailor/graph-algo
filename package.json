    "apollo-cache-inmemory": "^1.5.1",
    "apollo-client": "^2.5.1",
    "apollo-link": "^1.2.11",
    "apollo-link-context": "^1.0.17",
    "apollo-link-error": "^1.1.10",
    "apollo-link-http": "^1.5.14",
    "apollo-link-ws": "^1.0.17",
    "apollo-server": "^2.4.8",
    "apollo-server-express": "^2.4.8",
    "apollo-utilities": "^1.2.1",
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "cheerio": "^1.0.0-rc.3",
    "connect-history-api-fallback": "^1.6.0",
    "cors": "^2.8.5",
    "cross-env": "^5.2.0",
    "d3": "^5.9.2",
    "del": "^4.1.1",
    "express": "^4.16.4",
    "express-http-proxy": "^1.5.1",
    "graphql": "^14.2.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.11",
    "material-design-icons": "^3.0.1",
    "material-design-lite": "^1.3.0",
    "mathjs": "^5.9.0",
    "memoizee": "^0.4.14",
    "moment": "^2.24.0",
    "page": "^1.11.4",
    "pg": "^7.9.0",
    "pg-hstore": "^2.3.2",
    "rxjs": "^6.4.0",
    "sequelize": "^5.2.5",
