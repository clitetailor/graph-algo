{
  "name": "graph-algo",
  "version": "1.0.0",
  "scripts": {
    "start": "cross-env NODE_ENV=production run-s web:build server:start",
    "dev": "run-p web:autobuild server:dev",
    "server:start": "node server/index.js",
    "server:dev": "nodemon --watch server -q server/index.js",
    "web:dev": "npm-run-all web:autobuild",
    "web:build": "rollup -c rollup.config.js",
    "web:autobuild": "rollup -c rollup.config.js -w",
    "database:migrate": "sequelize db:migrate",
    "prettier": "prettier --config .prettierrc.yaml"
  },
  "main": "index.js",
  "repository": "https://github.com/clitetailor/graph-algo.git",
  "author": "clitetailor <ducnhatphung@gmail.com>",
  "license": "MIT",
